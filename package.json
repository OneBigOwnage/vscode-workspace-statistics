{
	"name": "workspace-statistics",
	"displayName": "Workspace statistics",
	"description": "Provides code statistics about your workspace.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:workspace-statistics.show-workspace-statistics",
		"onCommand:workspace-statistics.show-file-statistics"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "workspace-statistics.show-workspace-statistics",
				"title": "Show workspace statistics"
			}, {
				"command": "workspace-statistics.show-file-statistics",
				"title": "Show file statistics"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	}
}
